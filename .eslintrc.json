{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "google"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "object-curly-spacing": [2, "always"],
    "require-jsdoc": 0,
    "semi": [2, "never"],
    "comma-dangle": [2, "never"],
    "object-curly-spacing": [2, "always"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2
  }
}
