{
  "name": "ruhig",
  "version": "0.1.0",
  "description": "Release announcement header image generator",
  "scripts": {
    "start": "npm run watch:css & npm run watch:js & npm run serve",
    "serve": "browser-sync start -s --ss public -f 'public/**' --no-notify --no-open",
    "build:js": "webpack",
    "build:js:prod": "NODE_ENV=production webpack",
    "build:css": "postcss styles/style.css -o public/style.css -u postcss-import postcss-simple-vars postcss-nested",
    "build:css:prod": "npm run build:css -- -u autoprefixer cssnano --no-map",
    "watch:js": "webpack -w",
    "watch:css": "npm run build:css -- -w"
  },
  "author": "Hiroki Ishiwatari",
  "license": "MIT",
  "dependencies": {
    "install": "^0.10.1",
    "npm": "^5.0.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-promise": "^0.5.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.12",
    "cssnano": "^3.10.0",
    "eslint": "^4.1.1",
    "eslint-config-google": "^0.8.0",
    "eslint-plugin-react": "^7.1.0",
    "postcss": "^6.0.6",
    "postcss-cli": "^4.1.0",
    "postcss-import": "^10.0.0",
    "postcss-nested": "^2.0.2",
    "postcss-simple-vars": "^4.0.0",
    "webpack": "^3.0.0",
    "webpack-merge": "^4.1.0"
  },
  "browserslist": [
    "ie >= 11",
    "last 2 versions"
  ]
}
